urxvt*termName: rxvt-256color
urxvt*background: #000000
urxvt*foreground: #cccccc
#define termfont xft:xos4 Terminus:size=9:antialias=true
!#define termfont xft:Hack:size=9:antialias=true
urxvt*font: termfont
urxvt*boldFont: termfont
urxvt*italicFont: termfont
urxvt*boltItalicFont: termfont
!urxvt*letterSpace: -1

urxvt*scrollBar: false

urxvt*transparent: true
urxvt*shading: 20

